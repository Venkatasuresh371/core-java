package examples;

public class Test1
{
	public static void main(String[] args)
	{
		char c = 'x';
		switch (c) 
		{
		case 'a':System.out.println("hi suresh");
		break;
		case 'b':System.out.println("hi karthik");
		break;
		case 'c':System.out.println("hi yashwanth");
		break;
		default:System.out.println("default");
		}
	}
}